{
  "name": "blackhole-dl",
  "version": "0.0.0",
  "description": "A CLI for downloading files from BlackHole",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/blackhole-dl",
  "bugs": "https://github.com/vinsonchuong/blackhole-dl/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/blackhole-dl",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "bin": {
    "blackhole-dl": "./bin.js"
  },
  "dependencies": {
    "heliograph": "^5.0.0",
    "ink": "^3.0.8",
    "puppet-strings": "^4.0.1",
    "puppet-strings-chrome": "^2.0.1",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "semantic-release": "^17.4.2",
    "xo": "^0.38.2"
  },
  "ava": {
    "verbose": true,
    "timeout": "60s"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
